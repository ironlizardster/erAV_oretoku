;-------------------------------------------------
;挿入Ｇスポ責め
;-------------------------------------------------
@COM120
TFLAG:３Ｐ継続中 = 0
IF SELECTCOM == 34
	PRINTL 騎乗Ｇスポ責め
	SAVESTR:0 = 騎乗Ｇスポ責め
ELSE
	PRINTL 挿入Ｇスポ責め
	SAVESTR:0 = 挿入Ｇスポ責め
ENDIF

SELECTCOM = 120
CALL TRAIN_MESSAGE_B

;V経験を伴うコマンドのフラグ
TFLAG:V経験を伴うコマンド = 1
SIF TALENT:恋心 && ASSIPLAY == 0 && EXP:Ｖ経験 == 0
	TFLAG:主人による処女喪失 = 1

;調教者の射精チェック
CALL COM_EJAC_PLAYER_SEX

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
IF PREVCOM == 34
	LOSEBASE:体力 += 60
	LOSEBASE:気力 += 150
	SOURCE:12 = 1300
ELSE
	LOSEBASE:体力 += 50
	LOSEBASE:気力 += 100
	SOURCE:12 = 900
ENDIF

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:1 = 150
	SOURCE:3 = 100
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:1 = 200
	SOURCE:3 = 180
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:1 = 600
	SOURCE:3 = 250
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:1 = 2000
	SOURCE:3 = 380
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:1 = 2600
	SOURCE:3 = 500
ELSE
	SOURCE:1 = 3400
	SOURCE:3 = 800
ENDIF

;EXP:Ｖ経験をみる
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 = 300
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 = 100
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:6 = 10
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:6 = 0
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:1 , 1.30
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:1 , 1.80
	SOURCE:6 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:6 += 1000
	TIMES SOURCE:6 , 3.00
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:1 , 0.40
	SOURCE:6 += 300
	TIMES SOURCE:6 , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:6 , 0.20
ELSE
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:6 , 0.10
ENDIF

;対面座位だと効果が低い
IF PREVCOM == 22
	TIMES SOURCE:1 , 0.70
	TIMES SOURCE:3 , 0.90
;背面座位だと効果が高い
ELSEIF PREVCOM == 23
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:3 , 1.10
ENDIF

;調教者が[オトコ]
SIF TALENT:PLAYER:オトコ
	TIMES SOURCE:1 , 2.50

;小柄体形
SIF TALENT:小柄
	TIMES SOURCE:6 , 2.00

;貞操観念
IF TALENT:貞操観念
	IF TALENT:処女 == 1
		TIMES SOURCE:3 , 0.60
		TIMES SOURCE:14, 5.00
		SOURCE:15 = 2000
	ELSE
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 1000
	ENDIF
;貞操無頓着
ELSEIF TALENT:貞操無頓着
	IF TALENT:処女 == 1
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 300
	ENDIF
ELSE
	SIF TALENT:処女 == 1
		SOURCE:15 = 3000
ENDIF

;PALAM:欲情をみる
IF PALAM:抑鬱 < PALAMLV:1
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:3 , 0.30
ELSEIF PALAM:抑鬱 < PALAMLV:2
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.60
ELSEIF PALAM:抑鬱 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF PALAM:抑鬱 < PALAMLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:3 , 1.50
ELSE
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 1.80
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:3 , 0.60
	TIMES SOURCE:15 , 2.00
ELSEIF ABL:信頼 == 1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.80
	TIMES SOURCE:15 , 1.50
ELSEIF ABL:信頼 == 2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:15 , 1.00
ELSEIF ABL:信頼 == 3
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:15 , 0.80
ELSEIF ABL:信頼 == 4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:15 , 0.60
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:3 , 1.60
	TIMES SOURCE:15 , 0.30
ENDIF

IF PREVCOM == 34
	;ABL:奉仕精神をみる
	IF ABL:奉仕技術 == 0
		SOURCE:4 = 200
		SOURCE:5 = 50
		SOURCE:14 = 300
	ELSEIF ABL:奉仕技術 == 1
		SOURCE:4 = 250
		SOURCE:5 = 200
		SOURCE:14 = 100
	ELSEIF ABL:奉仕技術 == 2
		SOURCE:4 = 350
		SOURCE:5 = 550
		SOURCE:14 = 30
	ELSEIF ABL:奉仕技術 == 3
		SOURCE:4 = 450
		SOURCE:5 = 900
		SOURCE:14 = 0
	ELSEIF ABL:奉仕技術 == 4
		SOURCE:4 = 600
		SOURCE:5 = 1500
		SOURCE:14 = 0
	ELSE
		SOURCE:4 = 750
		SOURCE:5 = 2200
		SOURCE:14 = 0
	ENDIF
	;ABL:技巧をみる
	IF ABL:技巧 == 0
		TIMES SOURCE:1 , 0.30
		TIMES SOURCE:3 , 0.30
	ELSEIF ABL:技巧 == 1
		TIMES SOURCE:1 , 0.60
		TIMES SOURCE:3 , 0.60
	ELSEIF ABL:技巧 == 2
		TIMES SOURCE:1 , 1.00
		TIMES SOURCE:3 , 1.00
	ELSEIF ABL:技巧 == 3
		TIMES SOURCE:1 , 1.10
		TIMES SOURCE:3 , 1.10
	ELSEIF ABL:技巧 == 4
		TIMES SOURCE:1 , 1.30
		TIMES SOURCE:3 , 1.20
	ELSE
		TIMES SOURCE:1 , 1.50
		TIMES SOURCE:3 , 1.40
	ENDIF
ENDIF

;乱れ牡丹からの派生
IF PREVCOM == 612
	;ABL:Ｂ感覚をみる
	IF ABL:Ｂ感覚 == 0
		SOURCE:17 = 50
		SOURCE:3 += 50
		SOURCE:4 += 50
	ELSEIF ABL:Ｂ感覚 == 1
		SOURCE:17 = 200
		SOURCE:3 += 200
		SOURCE:4 += 200
	ELSEIF ABL:Ｂ感覚 == 2
		SOURCE:17 = 500
		SOURCE:3 += 500
		SOURCE:4 += 400
	ELSEIF ABL:Ｂ感覚 == 3
		SOURCE:17 = 1000
		SOURCE:3 += 600
		SOURCE:4 += 600
	ELSEIF ABL:Ｂ感覚 == 4
		SOURCE:17 = 1600
		SOURCE:3 += 1000
		SOURCE:4 += 1000
	ELSE
		SOURCE:3 = 2100
		SOURCE:3 += 1400
		SOURCE:4 += 1400
	ENDIF

	;ABL:Ｃ感覚をみる
	IF ABL:Ｃ感覚 == 0
		SOURCE:0 += 40
		SOURCE:4 += 50
	ELSEIF ABL:Ｃ感覚 == 1
		SOURCE:0 += 160
		SOURCE:4 += 200
	ELSEIF ABL:Ｃ感覚 == 2
		SOURCE:0 += 700
		SOURCE:4 += 400
	ELSEIF ABL:Ｃ感覚 == 3
		SOURCE:0 += 1500
		SOURCE:4 += 600
	ELSEIF ABL:Ｃ感覚 == 4
		SOURCE:0 += 2400
		SOURCE:4 += 1000
	ELSE
		SOURCE:0 += 3600
		SOURCE:4 += 1400
	ENDIF

	;プレイヤーのABL:技巧をみる
	IF ABL:PLAYER:技巧 == 0
		SOURCE:0 += 100
		SOURCE:1 += 100
		SOURCE:17 += 50
		SOURCE:3 += 100
		SOURCE:4 += 50
		SOURCE:10 += 0
		SOURCE:11 += 0
		A = 0
	ELSEIF ABL:PLAYER:技巧 == 1
		SOURCE:0 += 200
		SOURCE:1 += 200
		SOURCE:17 += 150
		SOURCE:3 += 150
		SOURCE:4 += 100
		SOURCE:10 += 50
		SOURCE:11 += 100
		A = 0
	ELSEIF ABL:PLAYER:技巧 == 2
		SOURCE:0 += 300
		SOURCE:1 += 300
		SOURCE:17 += 250
		SOURCE:3 += 200
		SOURCE:4 += 200
		SOURCE:10 += 100
		SOURCE:11 += 200
		A = 50
	ELSEIF ABL:PLAYER:技巧 == 3
		SOURCE:0 += 500
		SOURCE:1 += 400
		SOURCE:17 += 500
		SOURCE:3 += 300
		SOURCE:4 += 300
		SOURCE:10 += 150
		SOURCE:11 += 300
		A = 100
	ELSEIF ABL:PLAYER:技巧 == 4
		SOURCE:0 += 800
		SOURCE:1 += 500
		SOURCE:17 += 800
		SOURCE:3 += 500
		SOURCE:4 += 400
		SOURCE:10 += 250
		SOURCE:11 += 400
		A = 300
	ELSE
		SOURCE:0 += 1200
		SOURCE:1 += 600
		SOURCE:17 += 1200
		SOURCE:3 += 800
		SOURCE:4 += 600
		SOURCE:10 += 400
		SOURCE:11 += 500
		A = 500
	ENDIF

	;ABL:露出癖をみる
	IF ABL:性交技術 == 0
		SOURCE:7 += 100
		SOURCE:10 += 100
		TIMES A, 0.60
	ELSEIF ABL:性交技術 == 1
		SOURCE:7 += 200
		SOURCE:10 += 200
		TIMES A, 1.00
	ELSEIF ABL:性交技術 == 2
		SOURCE:7 += 400
		SOURCE:10 += 400
		TIMES A, 1.50
	ELSEIF ABL:性交技術 == 3
		SOURCE:7 += 700
		SOURCE:10 += 700
		TIMES A, 2.40
	ELSEIF ABL:性交技術 == 4
		SOURCE:7 += 1200
		SOURCE:10 += 1200
		TIMES A, 3.60
	ELSE
		SOURCE:7 += 2000
		SOURCE:10 += 2000
		TIMES A, 5.80
	ENDIF

	SOURCE:0 += A
	SOURCE:1 += A
	SOURCE:3 += A

	SIF EXP:Ｖ経験 >= EXPLV:3
		SOURCE:1 += A

	;TALENT:恋心をみる
	SIF TALENT:恋心
		TIMES SOURCE:3 , 2.00
ENDIF

;セックス後の処理
CALL COM_AFTER_VAGINA_SEX

RETURN 1
